<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="ParallelTests" parallel="tests" thread-count="2">
  <test thread-count="5" name="ChromeTest">
  <parameter name="bname" value="Chrome"></parameter>
    <classes>
      <class name="com.seleniumGrid.Parallel_Test"/>
    </classes>
  </test> <!-- Test -->
  
  
  <test thread-count="5" name="FirefoxTest">
  <parameter name="bname" value="Firefox"></parameter>
    <classes>
      <class name="com.seleniumGrid.Parallel_Test"/>
    </classes>
  </test> <!-- Test -->
  
  <test thread-count="5" name="EdgeTest">
  <parameter name="bname" value="Edge"></parameter>
    <classes>
      <class name="com.seleniumGrid.Parallel_Test"/>
    </classes>
  </test> <!-- Test -->
  
  
</suite> <!-- Suite -->
